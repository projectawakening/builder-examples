{
  "name": "contracts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "mud build",
    "clean": "forge clean && rimraf src/codegen",
    "deploy:local": "pnpm run build && mud deploy",
    "deploy:evenet": "pnpm run build && mud deploy --profile=evenet",
    "dev": "pnpm mud dev-contracts",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test": "tsc --noEmit && mud test",
    "configure-ratio": ". ./.env && pnpm forge script ./script/ConfigureRatio.s.sol:ConfigureRatio --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "execute": ". ./.env && pnpm forge script ./script/ExecuteVendingMachine.s.sol:ExecuteVendingMachine --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "configure-lens-seller": ". ./.env && pnpm forge script ./script/ConfigureLensSeller.s.sol:ConfigureLensSeller --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "approve": ". ./.env && pnpm forge script ./script/ApproveToken.s.sol:ApproveToken --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "purchaseLens": ". ./.env && pnpm forge script ./script/PurchaseLensSeller.s.sol:PurchaseLensSeller --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv"
  },
  "dependencies": {
    "@eveworld/common-constants": "^0.0.2",
    "@eveworld/world": "^0.0.2",
    "@latticexyz/cli": "2.0.0-transaction-context-98ef570f",
    "@latticexyz/schema-type": "2.0.0-transaction-context-98ef570f",
    "@latticexyz/store": "2.0.0-transaction-context-98ef570f",
    "@latticexyz/world": "2.0.0-transaction-context-98ef570f",
    "@latticexyz/world-modules": "2.0.0-transaction-context-98ef570f",
    "i": "^0.3.7",
    "npm": "^10.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "1.1.3",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.0.0-transaction-context-98ef570f",
    "solhint-plugin-mud": "2.0.0-transaction-context-98ef570f"
  }
}